<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}{% endblock %}
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
            <link href="{{ asset('css/style.css') }}" rel="stylesheet" type="text/css" />
            <link rel="shortcut icon" type="image/png" href="{{ asset('../asset/img/logo-car.png') }}"/>
        

        {% block javascripts %}
        <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
        {% endblock %}
    </head>
    <body>
    
        {% block body %} 
        {# navigation #}
<nav class="navbar navbar-expand-lg navbar-light bg-light" style="border-bottom:black 4px solid;" id="top">
  <a href="{{ path('home') }}"><img src="{{ asset('../asset/img/logo-car.png') }}" alt="logo" class="logo-nav navbar-brand"></a>
  

  <div class="collapse navbar-collapse" id="navbarSupportedContent" >
    <ul class="navbar-nav ml-auto">
      <li class="nav-item">
        <a class="nav-link nav_hover" href="{{ path('vehicule_index') }}">stock</a>
      </li>
      <li class="nav-item nav_hover">
        <a class="nav-link " href="{{ path('search') }}">rechercher</a>
      </li>
      
    </ul>
  
  </div>
</nav>
<div class="jumbotron jumbotron-fluid" style="border-bottom:black 4px solid;">
      
      <img src="{{ asset('../asset/img/LOGO-CAR-TOOL.jpg') }}" alt="logo" class="img-logo">
        
</div>


        {% endblock %}
    </body>
</html>
